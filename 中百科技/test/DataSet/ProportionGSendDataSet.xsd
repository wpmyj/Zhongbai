                    <CommandText>SELECT  sendProportion.inputDate, sendProportion.inputMan, sendProportion.remark, productModel.model, productName.name, productKind.sort, basicInfo.dasherName, 
               equipmentInformation.no, proportion.id
FROM     sendProportion INNER JOIN
               sendProportionDetail ON sendProportion.id = sendProportionDetail.spId INNER JOIN
               equipmentInformation ON sendProportionDetail.eiId = equipmentInformation.id INNER JOIN
               proportion ON sendProportionDetail.pId = proportion.id INNER JOIN
