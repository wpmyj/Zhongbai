                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  proportionDetail.id, proportionDetail.proportionValue, materialKind.sort, materialModel.model, materialName.name, proportion.id AS Expr1
FROM     proportion INNER JOIN
               proportionDetail INNER JOIN
               material ON proportionDetail.mId = material.id INNER JOIN
               materialModel ON material.mmId = materialModel.id INNER JOIN
               materialName ON material.mnId = materialName.id INNER JOIN
               materialKind ON materialName.mkId = materialKind.id ON proportion.id = proportionDetail.pId INNER JOIN
               sendProportionDetail INNER JOIN
