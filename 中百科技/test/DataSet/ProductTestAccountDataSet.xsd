                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  productTestAccount.id, CONVERT(varchar(10), productTestAccount.accountDate, 120) AS 台帐日期, productKind.id AS pkId, productKind.sort AS 产品种类, 
               productName.id AS pnId, productName.name AS 产品名称, productModel.id AS pmId, productModel.model AS 产品规格, productTestAccount.dayQuantity AS 当日产量, 
               productTestAccount.totalQuantity AS 累计产量, productTestAccount.no AS 试验编号, productTestAccount.name AS 试验名称, productTestAccount.man AS 试验员, 
               productTestAccount.result AS 试验结果, CASE mark WHEN 'false' THEN '否' WHEN 'true' THEN '是' END AS 是否合格, productTestAccount.frequency AS 实际频率, 
               productTestAccount.remark AS 备注, productTestAccount.inputDate AS 录入时间, productTestAccount.inputMan AS 录入人员, productTestGuideline.testGuideline
FROM     productTestAccount INNER JOIN
               product ON productTestAccount.pId = product.id INNER JOIN
               productName ON product.pnId = productName.id INNER JOIN
               productModel ON product.pmId = productModel.id INNER JOIN
               productKind ON productName.pkId = productKind.id INNER JOIN
               productTestGuideline ON product.id = productTestGuideline.pId
WHERE  (productTestAccount.accountDate &gt;= @date) AND (productTestAccount.accountDate &lt; DATEADD(day, 1, @date))</CommandText>
