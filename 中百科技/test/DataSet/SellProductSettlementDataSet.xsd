                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  sellProductSettlement.id AS 结算单号, sellProductSettlement.scId AS 合同编号, productKind.sort, productName.name, productModel.model, 
               sellProductSettlementDetail.count AS 验收数量, invoice.unitPrice, sellProductSettlementDetail.sum, basicInfo.dasherName
FROM     invoice INNER JOIN
               productModel INNER JOIN
               product ON productModel.id = product.pmId INNER JOIN
               productName ON product.pnId = productName.id INNER JOIN
               productKind ON productName.pkId = productKind.id ON invoice.pId = product.id INNER JOIN
               sellProductSettlement INNER JOIN
               sellProductSettlementDetail ON sellProductSettlement.id = sellProductSettlementDetail.spsId ON invoice.id = sellProductSettlementDetail.iId INNER JOIN
               sellContract ON sellProductSettlement.scId = sellContract.id AND invoice.scId = sellContract.id INNER JOIN
               clientInfo ON sellContract.ciId = clientInfo.id CROSS JOIN
               basicInfo
WHERE  (sellProductSettlement.id = @id)</CommandText>
