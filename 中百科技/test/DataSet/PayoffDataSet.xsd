                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select d.name as cHumanName,c.name as cPayRollName,sum(money)as realInCome FROM payOffDetail b 
		join payOffGather a on a.id = b.pogid
		join PayrollAdd c on b.paid = c.id
		join personnelInfo d on b.piid = d.id
	where (b.month = @month and year(a.date)=@year) 	
	group by d.name,c.name
union
select d.name,'实际所得',sum(realIncome) from payOffGather a, personnelInfo d 
	where (month(a.date)=@month and year(a.date)=@year) 
	and a.piId = d.id 		
group by d.name</CommandText>
