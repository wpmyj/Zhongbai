          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DasherStationConnetionString</DataSourceName>
        <CommandText>SELECT pl.id, pl.produceDate, en.name AS ename, pl.startMachineTime, 
      pl.stopMachineTime, pi.name AS piname, pt.name AS ptname, pl.weather, ei.no, 
      pl.remark
FROM againCalefactionProduceLog AS pl INNER JOIN
      equipmentInformation AS ei ON pl.eiId = ei.id INNER JOIN
      personnelInfo AS pi ON pl.piId = pi.id INNER JOIN
      produceTeam AS pt ON pl.ptId = pt.id INNER JOIN
      equipmentName AS en ON ei.enId = en.id
WHERE (pl.produceDate BETWEEN @start AND @end)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
